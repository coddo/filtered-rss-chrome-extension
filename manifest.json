{
    "manifest_version": 2,
    "name": "Filtered RSS feed viewer",
    "version": "1.0",
    "description": "Filter your RSS feeds to only see what you care about",

    "browser_action": {
        "default_icon": "icon.png",
        "default_popup": "index.html",
        "default_title": "Your filtered RSS feeds"
    },
    "permissions": [
        "activeTab",
        "storage"
    ]
}