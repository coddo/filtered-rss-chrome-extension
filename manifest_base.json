{
    "manifest_version": 2,
    "version": "1.0.1",
    "version_name": "1.0 beta-1",
    "name": "Fi.RSS",
    "description": "Filter your RSS feeds to only see what you care about",

    "browser_action": {
        "default_icon": "favicon.png",
        "default_popup": "index.html",
        "default_title": "Your filtered RSS feeds"
    },
    "permissions": [
        "storage",
        "notifications",
        "alarms"
    ],
    "background": {
        "scripts": [#{scripts_list}],
        "persistent": false
    },
    "icons": {
        "16": "favicon.png",
        "48": "favicon.png",
        "128": "favicon.png"
    }
}