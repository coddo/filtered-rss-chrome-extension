{
    "manifest_version": 2,
    "version": "1.0.2",
    "version_name": "1.0-beta1",
    "name": "Fi.RSS",
    "description": "Filter your RSS feeds to only see what you care about",

    "content_security_policy": "script-src 'self' 'sha256-PlodC6zWz8v1b2TGEEV7cbrphf4VmV64jB1OBM4oB44='; object-src 'self'",
    "browser_action": {
        "default_icon": "favicon.png",
        "default_popup": "index.html",
        "default_title": "Your filtered RSS feeds"
    },
    "permissions": [
        "storage",
        "notifications",
        "alarms",
        "https://vzir4xjww6.execute-api.eu-west-1.amazonaws.com/live/Fi-RSS-Reverse-Proxy"
    ],
    "background": {
        "scripts": [#{scripts_list}],
        "persistent": false
    },
    "icons": {
        "16": "favicon.png",
        "48": "favicon.png",
        "128": "favicon.png"
    }
}